select namealbums, releaseyearalbums from albums
	where releaseyearalbums = 2018;
	
select nametracks, greatest(duration) from tracks limit 1;

select nametracks from tracks 
	where duration between '00:03:30' and '00:59:00';
	
SELECT namemixtapes from mixtapes
	where releaseyearmixtapes between '2018' and '2020';
